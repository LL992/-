		<property name="com" value="10" />
	
	</bean> -->
	<!--  패키지명까지 적는이유는 동명이인방지 -->
	<!-- ExamConsole console = new GridExamConsloe(); -->
	<bean id="console" class="spring.di.ui.InlineExamConsole" >
